ENV=production

# Log level
LOG_LEVEL=DEBUG

# Local file logging
LOG_FILE_ENABLED=true
LOG_FILE_NAME=lab_app.log
LOG_FILE_MAX_BYTES=10485760
LOG_FILE_BACKUP_COUNT=5

# MinIO logging
LOG_MINIO_ENABLED=true
LOG_MINIO_BUCKET=lab-logs
LOG_MINIO_EXPIRY_DAYS=30
LOG_MINIO_ENDPOINT=http://localhost:9000
LOG_MINIO_ACCESS_KEY=minioadmin
LOG_MINIO_SECRET_KEY=minioadmin
LOG_MINIO_SECURE=false  