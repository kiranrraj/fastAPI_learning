[
    {
        "group_name": "Biochemistry",
        "tests": [
            {"name": "Blood Glucose (Fasting)", "lower": 70, "upper": 100, "unit": "mg/dL"},
            {"name": "Blood Glucose (Postprandial)", "lower": 70, "upper": 140, "unit": "mg/dL"},
            {"name": "Blood Urea", "lower": 10, "upper": 50, "unit": "mg/dL"},
            {"name": "Serum Creatinine", "lower": 0.6, "upper": 1.3, "unit": "mg/dL"},
            {"name": "Serum Sodium", "lower": 135, "upper": 145, "unit": "mmol/L"},
            {"name": "Serum Potassium", "lower": 3.5, "upper": 5.1, "unit": "mmol/L"}
        ]
    },
    {
        "group_name": "Liver Function",
        "tests": [
            {"name": "SGPT (ALT)", "lower": 7, "upper": 56, "unit": "U/L"},
            {"name": "SGOT (AST)", "lower": 5, "upper": 40, "unit": "U/L"},
            {"name": "Total Bilirubin", "lower": 0.3, "upper": 1.2, "unit": "mg/dL"},
            {"name": "Direct Bilirubin", "lower": 0.0, "upper": 0.3, "unit": "mg/dL"},
            {"name": "Alkaline Phosphatase", "lower": 40, "upper": 129, "unit": "U/L"},
            {"name": "Albumin", "lower": 3.4, "upper": 5.4, "unit": "g/dL"}
        ]
    },
    {
        "group_name": "Thyroid Profile",
        "tests": [
            {"name": "TSH", "lower": 0.4, "upper": 4.0, "unit": "µIU/mL"},
            {"name": "T3", "lower": 80, "upper": 200, "unit": "ng/dL"},
            {"name": "T4", "lower": 5.0, "upper": 12.0, "unit": "µg/dL"},
            {"name": "Free T3", "lower": 2.3, "upper": 4.2, "unit": "pg/mL"},
            {"name": "Free T4", "lower": 0.8, "upper": 1.8, "unit": "ng/dL"}
        ]
    },
    {
        "group_name": "Lipid Profile",
        "tests": [
            {"name": "Total Cholesterol", "lower": 125, "upper": 200, "unit": "mg/dL"},
            {"name": "HDL Cholesterol", "lower": 40, "upper": 60, "unit": "mg/dL"},
            {"name": "LDL Cholesterol", "lower": 0, "upper": 130, "unit": "mg/dL"},
            {"name": "Triglycerides", "lower": 0, "upper": 150, "unit": "mg/dL"},
            {"name": "VLDL", "lower": 5, "upper": 40, "unit": "mg/dL"}
        ]
    },
    {
        "group_name": "Renal Profile",
        "tests": [
            {"name": "Serum Uric Acid", "lower": 3.5, "upper": 7.2, "unit": "mg/dL"},
            {"name": "Calcium", "lower": 8.5, "upper": 10.5, "unit": "mg/dL"},
            {"name": "Phosphorus", "lower": 2.5, "upper": 4.5, "unit": "mg/dL"},
            {"name": "Chloride", "lower": 96, "upper": 106, "unit": "mmol/L"},
            {"name": "Bicarbonate", "lower": 22, "upper": 29, "unit": "mmol/L"}
        ]
    },
    {
        "group_name": "Hematology",
        "tests": [
            {"name": "Hemoglobin", "lower": 13.5, "upper": 17.5, "unit": "g/dL"},
            {"name": "WBC Count", "lower": 4500, "upper": 11000, "unit": "/µL"},
            {"name": "RBC Count", "lower": 4.7, "upper": 6.1, "unit": "million/µL"},
            {"name": "Platelet Count", "lower": 150000, "upper": 450000, "unit": "/µL"},
            {"name": "Hematocrit", "lower": 38.8, "upper": 50, "unit": "%"}
        ]
    },
    {
        "group_name": "Diabetes Panel",
        "tests": [
            {"name": "HbA1c", "lower": 4.0, "upper": 5.6, "unit": "%"},
            {"name": "Fasting Glucose", "lower": 70, "upper": 100, "unit": "mg/dL"},
            {"name": "Insulin (Fasting)", "lower": 2.0, "upper": 25.0, "unit": "µIU/mL"},
            {"name": "C-Peptide", "lower": 0.78, "upper": 1.89, "unit": "ng/mL"}
        ]
    },
    {
        "group_name": "Cardiac Markers",
        "tests": [
            {"name": "Troponin I", "lower": 0, "upper": 0.04, "unit": "ng/mL"},
            {"name": "CK-MB", "lower": 0, "upper": 5, "unit": "ng/mL"},
            {"name": "LDH", "lower": 140, "upper": 280, "unit": "U/L"},
            {"name": "BNP", "lower": 0, "upper": 100, "unit": "pg/mL"}
        ]
    },
    {
        "group_name": "Infectious Diseases",
        "tests": [
            {"name": "Widal Test", "lower": 0, "upper": 160, "unit": "titre"},
            {"name": "HIV (ELISA)", "lower": 0, "upper": 1, "unit": "index"},
            {"name": "Hepatitis B Surface Antigen (HBsAg)", "lower": 0, "upper": 0.05, "unit": "IU/mL"},
            {"name": "Dengue NS1 Antigen", "lower": 0, "upper": 1, "unit": "index"}
        ]
    },
    {
        "group_name": "Hormonal Assays",
        "tests": [
            {"name": "FSH", "lower": 1.5, "upper": 12.4, "unit": "mIU/mL"},
            {"name": "LH", "lower": 1.7, "upper": 8.6, "unit": "mIU/mL"},
            {"name": "Prolactin", "lower": 4.8, "upper": 23.3, "unit": "ng/mL"},
            {"name": "Testosterone", "lower": 300, "upper": 1000, "unit": "ng/dL"}
        ]
    }
]